{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions":{
    "target":"es6",
    "module":"commonjs",
    "declaration":false,
    "noLib":false,
    "emitDecoratorMetadata":true,
    "experimentalDecorators":true,
    "sourceMap":true,
    "pretty":true,
    "noImplicitAny":true,
    "noImplicitReturns":false,
    "noImplicitUseStrict":false,
    "baseUrl":"src/",
    "lib": [
      "dom"
    ]
  },
  "include":[
    "src/**/*"
  ],
  "exclude":[
    "node_modules"
  ],
  "compileOnSave":false
}
